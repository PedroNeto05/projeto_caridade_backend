{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2022",
    "paths": {
      "@/*": ["src/app/*"] 
    },
    "lib": ["ES2023"],
    "module": "node16",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": [
    "./node_modules/@types",
    "./src/@types"
  ],
  }
}
